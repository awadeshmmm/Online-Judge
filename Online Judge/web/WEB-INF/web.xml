<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>user.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_auth</servlet-name>
        <servlet-class>user.user_auth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>code_eval</servlet-name>
        <servlet-class>code_eval</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>account_details</servlet-name>
        <servlet-class>account_details</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_auth</servlet-name>
        <url-pattern>/user_auth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>code_eval</servlet-name>
        <url-pattern>/code_eval</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>account_details</servlet-name>
        <url-pattern>/account_details</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index1.jsp</welcome-file>
    </welcome-file-list>
</web-app>
